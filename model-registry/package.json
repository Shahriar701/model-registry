{
  "name": "model-registry",
  "version": "1.0.0",
  "description": "Serverless ML Model Registry for centralized model metadata management",
  "bin": {
    "model-registry": "bin/model-registry.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "deploy:dev": "ENVIRONMENT=dev cdk deploy --all",
    "deploy:prod": "ENVIRONMENT=prod cdk deploy --all",
    "destroy": "cdk destroy --all",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap",
    "clean": "rm -rf lib/ cdk.out/ coverage/ node_modules/.cache/",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run test && npm run build"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "aws-cdk": "2.100.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.2.0"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.400.0",
    "@aws-sdk/client-dynamodb": "^3.400.0",
    "@aws-sdk/client-eventbridge": "^3.400.0",
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/lib-dynamodb": "^3.400.0",
    "aws-cdk-lib": "2.100.0",
    "constructs": "^10.0.0",
    "zod": "^3.23.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "ml",
    "model-registry",
    "serverless",
    "infrastructure-as-code"
  ],
  "author": "Platform Engineering Team",
  "license": "MIT"
}
