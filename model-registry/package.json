{
  "name": "model-registry",
  "version": "1.0.0",
  "description": "ML Model Registry Service",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "./scripts/test-integration.sh",
    "test:smoke": "./scripts/test-integration.sh",
    "test:health": "curl -f $API_GATEWAY_URL/api/v1/health",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist lib/src",
