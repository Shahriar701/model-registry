  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "aws-cdk-lib": "^2.223.0",
    "constructs": "^10.4.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.0.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.0",
    "typescript": "^4.9.0"
